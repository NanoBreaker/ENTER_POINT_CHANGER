            <Button layoutX="402.0" layoutY="506.0" mnemonicParsing="false" onAction="#buttonPressed_FindSomething" prefHeight="38.0" prefWidth="377.0" text="Заменить поле &quot;ID Фильтра&quot; c &quot;1&quot; на &quot;4&quot;" />
